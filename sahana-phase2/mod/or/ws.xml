<?xml version="1.0" encoding="ISO-8859-1"?>
<web_services>
	<types>
		<type>
			  	<name>Basic</name>
    			<typeclass>complexType</typeclass>
    			<phptype>struct</phptype>
    			<compositor>all</compositor>
    			<restrictionBase></restrictionBase>
    			<elements>
    				<element type="xsd:string">name</element>
					<element type="xsd:string">id</element>
   				</elements>
   				<attrs></attrs>
   				<arrayType></arrayType>
		</type>
		<type>
			  	<name>BasicArray</name>
    			<typeclass>complexType</typeclass>
    			<phptype>array</phptype>
    			<compositor></compositor>
    			<restrictionBase>SOAP-ENC:Array</restrictionBase>
    			<elements></elements>
   				<attrs></attrs>
   				<arrayType>tns:Basic</arrayType>
		</type>
	</types>
 	<service>
 		<name>
 			List of Organizations (the number of orgs)
 		</name>
 		<api_function>
 			shn_or_organization_list
 		</api_function>
 		<input type="xsd:int">limit</input>
 		<output type="BasicArray" />
 		<documentation>
 			A useful method to retrieve a list of organizations with the name and UUID
 		</documentation>
  	</service>
 	<service>
 		<name>
 			List of services offered
 		</name>
 		<api_function>
 			shn_or_service_list
 		</api_function>
 		<input></input>
 		<output type="BasicArray" />
 		<documentation>
 			A useful method to retrieve a list of services , organizations can offer
 		</documentation>
 	</service>
 	<service>
 		<name>
 			List of organization types
 		</name>
 		<api_function>
 			shn_or_type_list
 		</api_function>
 		<input></input>
 		<output type="BasicArray" />
 		<documentation>
 			A useful method to retrieve the list of types ,by which organizations are grouped by
 		</documentation>
 	</service>
 </web_services>