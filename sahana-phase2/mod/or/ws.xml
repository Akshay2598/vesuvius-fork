<?xml version="1.0" encoding="ISO-8859-1"?>
<web_services>
	<types>
		<type>
			  	<name>Org</name>
    			<typeclass>complexType</typeclass>
    			<phptype>struct</phptype>
    			<compositor>all</compositor>
    			<restrictionBase></restrictionBase>
    			<elements>
   			 	array(
        			'name' => array('name'=>'name','type'=>'xsd:string'),
        			'o_uuid' => array('name'=>'o_uuid','type'=>'xsd:string')
   				)
   				</elements>
   				<attrs></attrs>
   				<arrayType></arrayType>
		</type>
		<type>
			  	<name>OrgArray</name>
    			<typeclass>complexType</typeclass>
    			<phptype>array</phptype>
    			<compositor></compositor>
    			<restrictionBase>SOAP-ENC:Array</restrictionBase>
    			<elements>
					array()
   				</elements>
   				<attrs>
   				    array(
        				array('ref'=>'SOAP-ENC:arrayType','wsdl:arrayType'=>'tns:Org[]')
    				)
   				</attrs>
   				<arrayType>tns:Org</arrayType>
		</type>
	</types>
 	<service>
 		<name>
 			List of Organizations (the number of orgs)
 		</name>
 		<api_function>
 			shn_or_org_list
 		</api_function>
 		<input type="string">profile</input>
 		<output type="OrgArray" />
  	</service>
 	<service>
 		<name>
 			List of services offered
 		</name>
 		<api_function>
 			shn_or_service_list
 		</api_function>
 		<input type="string">profile</input>
 		<output type="OrgArray" />
 	</service>
 </web_services>