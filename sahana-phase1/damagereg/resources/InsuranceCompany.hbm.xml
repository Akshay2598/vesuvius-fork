<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

  <class name="org.damage.business.InsuranceCompany" table="insurancecompany">
    <id name="InsuranceCompanyCode" column="InsuranceCompanyCode" type="long" unsaved-value="null">
      <generator class="increment"/>
    </id>
    <property name="InsuranceCompanyName" column="InsuranceCompanyName" type="string" length="45" />

  </class>

</hibernate-mapping>