<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

  <class name="org.damage.business.DamageDetailHospitalEstimatedCost" table="dmg_detail_hospital_est_cost">
    <id name="dmgDetailEstimatedCostId" column="DmgDetailEstimatedCostId" type="long" unsaved-value="null">
    <generator class="increment"/>
    </id>
    <property name="damageReportId" column="damageReportId" type="long" />
    <property name="PropertyId" column="PropertyId" type="long" />
	<property name="budgetDescription" column="BudgetDescription" type="string" length="200" />
    <property name="estimatedValue" column="EstimatedValue" type="double" />

  </class>

</hibernate-mapping>