<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

  <class name="org.damage.business.Institution" table="institution">
    <id name="institutionCode" column="institutionCode" type="long" unsaved-value="null">
      <generator class="increment"/>
    </id>
    <property name="institutionName" column="institutionName" type="string" length="45" />

  </class>

</hibernate-mapping>