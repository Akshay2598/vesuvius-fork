<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

  <class name="org.damage.business.DamageReportDetailHospital" table="dmg_detail_hospital">
    <id name="damageDetailHospitalId" column="DamageDetailHospitalId" type="long" unsaved-value="null">
    <generator class="increment"/>
    </id>
    <property name="propertyId" column="PropertyId" type="long" />
    <property name="damageReportId" column="DamageReportId" type="long" />
	<property name="PropertyTypeCode" column="PropertyTypeCode" type="string" length="200" />
    <property name="hospitalName" column="HospitalName" type="string" length="200" />
    <property name="summaryStatus" column="SummaryStatus" type="string" length="500" />
    <property name="summaryFacility" column="SummaryFacility" type="string" length="200" />

  </class>

</hibernate-mapping>